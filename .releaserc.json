{
  "branches": ["master"],
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "eslint",
        "releaseRules": [
          { "message": "FC", "release": "patch" },
          { "message": "fc", "release": "patch" }
        ]
      }
    ],
    ["semantic-release-jira-notes", {
      "jiraHost": "fordeer.atlassian.net",
      "ticketPrefixes": ["FC"]
    }],
    "@semantic-release/changelog",
    "@semantic-release/git",
    "@semantic-release/github"
  ]
}
